{
	"name": "archixl/sparql-result-format",
	"description": "MediaWiki extension that allows you to query SPARQL endpoints and visualize results in various formats",
	"license": "MIT",
	"type": "mediawiki-extension",
	"authors": [
		{
			"name": "Youri van den Bogert",
			"email": "yvdbogert@archixl.nl"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "^9.6",
		"php-parallel-lint/php-parallel-lint": "1.4.0",
		"php-parallel-lint/php-console-highlighter": "1.0.0",
		"mediawiki/mediawiki-codesniffer": "45.0.0",
		"mediawiki/minus-x": "1.1.3"
	},
	"scripts": {
		"test": [
			"phpcs -p -s",
			"minus-x check ."
		],
		"fix": [
			"minus-x fix .",
			"phpcbf"
		],
		"phpunit": "phpunit"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
